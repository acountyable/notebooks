{
  "name": "@findhow/notebooks",
  "version": "0.0.1",
  "description": "Notebooks for Findhow",
  "tasks": {
    "mock:api": "npm install -g @stoplight/prism-cli && prism mock ./openapi.yaml --port 4000",
    "start": "deno task jupyter:install && deno task jupyter:kernel && deno task jupyter",
    "jupyter:install": "deno jupyter --install",
    "jupyter:kernel": "jupyter notebook --kernel deno --unstable",
    "jupyter": "deno run --allow-env --allow-read --allow-net --allow-plugin --watch --unstable"
  },
  "exports": "./mod.ts",
  "nodeModulesDir": "auto",
  "imports": {
    "@std/archive": "jsr:@std/archive",
    "@std/assert": "jsr:@std/assert",
    "@std/bytes": "jsr:@std/bytes",
    "@std/cache": "jsr:@std/cache",
    "@std/cli": "jsr:@std/cli",
    "@std/crypto": "jsr:@std/crypto",
    "@std/csv": "jsr:@std/csv",
    "@std/data-structures": "jsr:@std/data-structures",
    "@std/datetime": "jsr:@std/datetime",
    "@std/dotenv": "jsr:@std/dotenv",
    "@std/encoding": "jsr:@std/encoding",
    "@std/expect": "jsr:@std/expect",
    "@std/fmt": "jsr:@std/fmt",
    "@std/front-matter": "jsr:@std/front-matter",
    "@std/fs": "jsr:@std/fs",
    "@std/html": "jsr:@std/html",
    "@std/http": "jsr:@std/http",
    "@std/ini": "jsr:@std/ini",
    "@std/io": "jsr:@std/io",
    "@std/json": "jsr:@std/json",
    "@std/jsonc": "jsr:@std/jsonc",
    "@std/log": "jsr:@std/log",
    "@std/media-types": "jsr:@std/media-types",
    "@std/msgpack": "jsr:@std/msgpack",
    "@std/net": "jsr:@std/net",
    "@std/path": "jsr:@std/path",
    "@std/random": "jsr:@std/random",
    "@std/regexp": "jsr:@std/regexp",
    "@std/semver": "jsr:@std/semver",
    "@std/streams": "jsr:@std/streams",
    "@std/tar": "jsr:@std/tar",
    "@std/testing": "jsr:@std/testing",
    "@std/text": "jsr:@std/text",
    "@std/toml": "jsr:@std/toml",
    "@std/ulid": "jsr:@std/ulid",
    "@std/uuid": "jsr:@std/uuid",
    "@std/webgpu": "jsr:@std/webgpu",
    "@std/yaml": "jsr:@std/yaml",
    "prisma": "npm:prisma@^5.20.0"
  }
}
